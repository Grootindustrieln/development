                                    </div>
                                </div>
                            </div>
                            <script>
                                $('#feedback-form').bootstrapValidator({
                                    live: 'submitted',
                                    fields: {
                                        feedback_details: {
                                            validators: {
                                                notEmpty: {
                                                    message: "Please enter your feedback"
                                                }
                                            }
                                        }
                                    }
                                });
                            </script>
                        </div>
                        <div class="modal-footer" id="feedback-modalFooter">
