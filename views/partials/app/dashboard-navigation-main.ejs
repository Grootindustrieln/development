        <script>
            $(document).ready(function() {
                $('[data-toggle="expand"]').click(function(e){
                    e.preventDefault()
                    $($(this).attr('href')).toggleClass('expand');
                    $.ajax({
                        url: '/app/cp/leftnav',
                        data: {'left_nav': $($(this).attr('href')).hasClass('expand')?1:0},
                        dataType: 'json',
                        method: "POST"
                    });
                });
                $('#support').on('show.bs.modal', function(e){
                    if(!$('#support .modal-body').html().length){
                        $('#support .modal-body').load('');
                    }
                });
                $('#support').on('hidden.bs.modal', function(e){
                    $('#support .modal-body').html('');
                });
                $('#feedback-form')
                   .bootstrapValidator()
                   .on('success.form.bv', function(event) {
                        var stackcenter = {dir1: "down", dir2: "right", dir3:"top"},
                            data = $(this).serialize();
                            data = data + "&feedback_type=General";
                            event.preventDefault();
                            // Send the email
                            $.ajax({
                                method: "POST",
                                url: "",
                                data: data
                            })
                            .done(function(){
                                $('#feedback-modal').modal('hide');
                                $('#feedback-confirm-modal').modal('show');
                                setTimeout(function() {
                                    $('#feedback-confirm-modal').modal('hide');
                                }, 5000);
                                $('#feedback-form').find('textarea').val('');
                                $('#feedback-form').find('input[type="checkbox"]').attr('checked', false);
                                $('#feedback-form').data('bootstrapValidator').resetForm();
                        });
                    });
                });
        </script>