    MktoForms2.whenReady( function(form) {

      form.onValidate( function() {

              var email = form.vals().Email.toLowerCase();
              var company = form.vals().Company.toLowerCase();

              if( email ) {

                  if( !isEmailGood(email) ) {

                    form.submittable(false);
                    var emailElem = form.getFormElem().find("#Email");
                    emailElem.addClass('mktoInvalid');
                    form.showErrorMessage("Please provide your Business email.", emailElem);
                    

                  } else {

                      form.submittable(true);

                  }
              }
          });
    });

    function isEmailGood(email) {

        for(var i = 0; i < invalidDomains.length; i++) {

            var domain = invalidDomains[i];

            if (email.indexOf(domain) != -1) {

                return false;

            }

        }

        return true;
    }

    function isCompanyGood(company) {

        for(var i = 0; i < invalidDomains.length; i++) {

            var domain = invalidDomains[i];

            if (company.indexOf(domain) != -1) {

                return false;

            }

        }

        return true;
    }

